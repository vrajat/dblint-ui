{
  "name": "dblint-ui",
  "version": "0.6.0",
  "private": true,
  "devDependencies": {
    "better-npm-run": "^0.1.1",
    "codecov": "^3.1.0",
    "cra-universal": "^4.1.0",
    "prettier": "1.14.2",
    "react-scripts": "2.1.1"
  },
  "dependencies": {
    "@cra-express/core": "^4.1.0",
    "@cra-express/redux-prefetcher": "^4.1.0",
    "@cra-express/router-prefetcher": "^4.1.0",
    "axios": "0.18.0",
    "babel-runtime": "^6.26.0",
    "bootstrap": "^4.1.2",
    "loadable-components": "2.2.3",
    "react": "^16.6.1",
    "react-bootstrap": "v1.0.0-beta.3",
    "react-dom": "^16.6.1",
    "react-fontawesome": "^1.6.1",
    "react-helmet-async": "0.1.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "crau:start": "better-npm-run crau:start",
    "crau:build": "better-npm-run crau:build",
    "crau:clean": "better-npm-run crau:clean",
    "start": "react-scripts start",
    "start:prod": "better-npm-run start:prod",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "build:all": "cra-universal build",
    "prettier": "prettier --write \"{src,__{tests,mocks}__}/**/*.js\"",
    "k8s": "VERSION=$VERSION GCLOUD_PROJECT=$GCLOUD_PROJECT sed \"s/\\[VERSION\\]/$VERSION/g;s/\\[GCLOUD_PROJECT\\]/$GCLOUD_PROJECT/g\" k8s.yml > k8s.tmpl.yml"
  },
  "betterScripts": {
    "start:prod": {
      "command": "node ./server/bundle",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "crau:start": {
      "command": "cra-universal start",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "crau:build": {
      "command": "cra-universal build",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "crau:clean": "cra-universal clean"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
